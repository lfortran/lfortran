#ifndef LCOMPILERS_CONFIG_H
#define LCOMPILERS_CONFIG_H

/* Define if you want to enable ASSERT testing in LCompilers */
#cmakedefine WITH_LCOMPILERS_ASSERT

/* LCompilers version */
#cmakedefine LCOMPILERS_VERSION "@LCOMPILERS_VERSION@"

/* Define if LLVM is enabled */
#cmakedefine HAVE_LCOMPILERS_LLVM

/* Define if RAPIDJSON is found */
#cmakedefine HAVE_LCOMPILERS_RAPIDJSON

/* Define if stacktrace is enabled */
#cmakedefine HAVE_LCOMPILERS_STACKTRACE
#cmakedefine HAVE_LCOMPILERS_BFD
#cmakedefine HAVE_LCOMPILERS_DWARFDUMP
#cmakedefine HAVE_LCOMPILERS_LINK
#cmakedefine HAVE_LCOMPILERS_MACHO
#cmakedefine HAVE_LCOMPILERS_UNWIND

/* Define if cxxabi.h is present */
#cmakedefine HAVE_LCOMPILERS_DEMANGLE

/* Define if XEUS is enabled */
#cmakedefine HAVE_LCOMPILERS_XEUS

/* Define if we should use binary modfiles */
#cmakedefine WITH_LCOMPILERS_BINARY_MODFILES

#endif // LCOMPILERS_CONFIG_H
