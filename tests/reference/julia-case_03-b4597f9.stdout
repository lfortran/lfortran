function main()
    local a::Int32 = 1
    local b::Int32 = 2
    local marks::Int32 = 0
    marks = 94
    if marks ≥ 40 + b
        println("Pass!")
    elseif marks ≤ 39 - a
        println("Failed!")
    else
        println("Invalid marks")
    end
    println("Your marks are ", " ", marks)
    marks = -1
    if marks ≥ 40 + b
        println("Pass!")
    elseif 0 ≤ marks ≤ 39 - a
        println("Failed!")
    else
        println("Invalid marks")
    end
    println("Your marks are ", " ", marks)
end

main()
