use num::complex::Complex;
use num::traits::Pow;
use ndarray::{array, Array1, Array2, Array3, Array4, Array5, Array6};

fn main() {
    let mut i: i32 = 0;
    let mut j: i32 = 0;
    i = 1;
    j = 0;
    while i < 11 {
        j = j + i;
        i = i + 1;
    }
    if j != 55 {
        eprintln!("ERROR STOP");
        std::process::exit(1);
    }
    if i != 11 {
        eprintln!("ERROR STOP");
        std::process::exit(1);
    }
    i = 1;
    j = 0;
    while i <= 10 {
        j = j + i;
        i = i + 1;
    }
    if j != 55 {
        eprintln!("ERROR STOP");
        std::process::exit(1);
    }
    if i != 11 {
        eprintln!("ERROR STOP");
        std::process::exit(1);
    }
    i = 1;
    j = 0;
    while i < 1 {
        j = j + i;
        i = i + 1;
    }
    if j != 0 {
        eprintln!("ERROR STOP");
        std::process::exit(1);
    }
    if i != 1 {
        eprintln!("ERROR STOP");
        std::process::exit(1);
    }
}
