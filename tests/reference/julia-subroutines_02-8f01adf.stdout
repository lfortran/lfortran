function f(a::Int32, b::Base.RefValue{Int32})
    b[] = a + 1
end

function g(a::Int32, b::Base.RefValue{Int32})
    b[] = a - 1
end

function h(a::Int32, b::Base.RefValue{Int32})
    g(a, b)
end

function main()
    local i::Int32
    local j::Int32
    i = 1
    j = 1
    __j_ref__= Ref(j)
    f(i, __j_ref__)
    j = __j_ref__[]
    println(i, " ", j)
    if i ≠ 1
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
    if j ≠ 2
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
    __j_ref__= Ref(j)
    g(i, __j_ref__)
    j = __j_ref__[]
    println(i, " ", j)
    if i ≠ 1
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
    if j ≠ 0
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
    __j_ref__= Ref(j)
    h(i, __j_ref__)
    j = __j_ref__[]
    println(i, " ", j)
    if i ≠ 1
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
    if j ≠ 0
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
end

main()
