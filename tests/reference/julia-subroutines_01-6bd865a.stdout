    println(Base.stderr, "ERROR STOP")
    exit(1);
end
__j_ref__= Ref(j)
f(i, __j_ref__)
j = __j_ref__[]
println(i, " ", j)
if i ≠ 1
    println(Base.stderr, "ERROR STOP")
    exit(1);
end
if j ≠ 2
    println(Base.stderr, "ERROR STOP")
    exit(1);
end
