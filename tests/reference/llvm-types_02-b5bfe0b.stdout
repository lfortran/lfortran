; ModuleID = 'LFortran'
source_filename = "LFortran"

define i32 @main(i32 %0, ptr %1) {
.entry:
  %i = alloca i32, align 4
  call void @_lpython_call_initial_functions(i32 %0, ptr %1)
  %i1 = alloca i32, align 4
  %r = alloca float, align 4
  store i32 1, ptr %i1, align 4
  store float 1.000000e+00, ptr %r, align 4
  %2 = load i32, ptr %i1, align 4
  %3 = sitofp i32 %2 to float
  store float %3, ptr %r, align 4
  call void @_lpython_free_argv()
  br label %return

return:                                           ; preds = %.entry
  br label %FINALIZE_SYMTABLE_types_02

FINALIZE_SYMTABLE_types_02:                       ; preds = %return
  ret i32 0
}

declare void @_lpython_call_initial_functions(i32, ptr)

declare void @_lpython_free_argv()
