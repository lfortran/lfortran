; ModuleID = 'LFortran'
source_filename = "LFortran"

%string_descriptor = type <{ ptr, i64 }>

@string_const_data = private constant [4 x i8] c"Mr. "
@string_const = private global %string_descriptor <{ ptr @string_const_data, i64 4 }>
@string_const_data.1 = private constant [6 x i8] c"Rowan "
@string_const.2 = private global %string_descriptor <{ ptr @string_const_data.1, i64 6 }>
@string_const_data.3 = private constant [8 x i8] c"Atkinson"
@string_const.4 = private global %string_descriptor <{ ptr @string_const_data.3, i64 8 }>
@string_const_data.5 = private constant [25 x i8] c"A big hello from Mr. Bean"
@string_const.6 = private global %string_descriptor <{ ptr @string_const_data.5, i64 25 }>
@0 = private unnamed_addr constant [2 x i8] c"\0A\00", align 1
@serialization_info = private unnamed_addr constant [38 x i8] c"S-DESC-8,S-DESC-6,S-DESC-15,S-DESC-15\00", align 1
@string_const_data.7 = private constant [8 x i8] c"Here is "
@string_const.8 = private global %string_descriptor <{ ptr @string_const_data.7, i64 8 }>
@1 = private unnamed_addr constant [5 x i8] c"%s%s\00", align 1
@2 = private unnamed_addr constant [2 x i8] c"\0A\00", align 1
@3 = private unnamed_addr constant [5 x i8] c"%s%s\00", align 1

define i32 @main(i32 %0, ptr %1) {
.entry:
  call void @_lpython_call_initial_functions(i32 %0, ptr %1)
  %firstname = alloca %string_descriptor, align 8
  store %string_descriptor zeroinitializer, ptr %firstname, align 1
  %2 = getelementptr %string_descriptor, ptr %firstname, i32 0, i32 1
  store i64 15, ptr %2, align 4
  %3 = getelementptr %string_descriptor, ptr %firstname, i32 0, i32 0
  %4 = call ptr @_lfortran_malloc(i64 15)
  store ptr %4, ptr %3, align 8
  %greetings = alloca %string_descriptor, align 8
  store %string_descriptor zeroinitializer, ptr %greetings, align 1
  %5 = getelementptr %string_descriptor, ptr %greetings, i32 0, i32 1
  store i64 25, ptr %5, align 4
  %6 = getelementptr %string_descriptor, ptr %greetings, i32 0, i32 0
  %7 = call ptr @_lfortran_malloc(i64 25)
  store ptr %7, ptr %6, align 8
  %surname = alloca %string_descriptor, align 8
  store %string_descriptor zeroinitializer, ptr %surname, align 1
  %8 = getelementptr %string_descriptor, ptr %surname, i32 0, i32 1
  store i64 15, ptr %8, align 4
  %9 = getelementptr %string_descriptor, ptr %surname, i32 0, i32 0
  %10 = call ptr @_lfortran_malloc(i64 15)
  store ptr %10, ptr %9, align 8
  %title = alloca %string_descriptor, align 8
  store %string_descriptor zeroinitializer, ptr %title, align 1
  %11 = getelementptr %string_descriptor, ptr %title, i32 0, i32 1
  store i64 6, ptr %11, align 4
  %12 = getelementptr %string_descriptor, ptr %title, i32 0, i32 0
  %13 = call ptr @_lfortran_malloc(i64 6)
  store ptr %13, ptr %12, align 8
  %14 = getelementptr %string_descriptor, ptr %title, i32 0, i32 0
  %15 = getelementptr %string_descriptor, ptr %title, i32 0, i32 1
  %16 = load ptr, ptr @string_const, align 8
  call void @_lfortran_strcpy(ptr %14, ptr %15, i8 0, i8 0, ptr %16, i64 4)
  %17 = getelementptr %string_descriptor, ptr %firstname, i32 0, i32 0
  %18 = getelementptr %string_descriptor, ptr %firstname, i32 0, i32 1
  %19 = load ptr, ptr @string_const.2, align 8
  call void @_lfortran_strcpy(ptr %17, ptr %18, i8 0, i8 0, ptr %19, i64 6)
  %20 = getelementptr %string_descriptor, ptr %surname, i32 0, i32 0
  %21 = getelementptr %string_descriptor, ptr %surname, i32 0, i32 1
  %22 = load ptr, ptr @string_const.4, align 8
  call void @_lfortran_strcpy(ptr %20, ptr %21, i8 0, i8 0, ptr %22, i64 8)
  %23 = getelementptr %string_descriptor, ptr %greetings, i32 0, i32 0
  %24 = getelementptr %string_descriptor, ptr %greetings, i32 0, i32 1
  %25 = load ptr, ptr @string_const.6, align 8
  call void @_lfortran_strcpy(ptr %23, ptr %24, i8 0, i8 0, ptr %25, i64 25)
  %26 = alloca i64, align 8
  %27 = load %string_descriptor, ptr %title, align 1
  %28 = alloca %string_descriptor, align 8
  store %string_descriptor %27, ptr %28, align 1
  %29 = load %string_descriptor, ptr %firstname, align 1
  %30 = alloca %string_descriptor, align 8
  store %string_descriptor %29, ptr %30, align 1
  %31 = load %string_descriptor, ptr %surname, align 1
  %32 = alloca %string_descriptor, align 8
  store %string_descriptor %31, ptr %32, align 1
  %33 = call ptr (ptr, i64, ptr, ptr, i32, i32, ...) @_lcompilers_string_format_fortran(ptr null, i64 0, ptr @serialization_info, ptr %26, i32 0, i32 0, ptr @string_const.8, ptr %28, ptr %30, ptr %32)
  %34 = load i64, ptr %26, align 4
  %stringFormat_desc = alloca %string_descriptor, align 8
  %35 = getelementptr %string_descriptor, ptr %stringFormat_desc, i32 0, i32 0
  store ptr %33, ptr %35, align 8
  %36 = getelementptr %string_descriptor, ptr %stringFormat_desc, i32 0, i32 1
  store i64 %34, ptr %36, align 4
  %37 = getelementptr %string_descriptor, ptr %stringFormat_desc, i32 0, i32 0
  %38 = load ptr, ptr %37, align 8
  %39 = getelementptr %string_descriptor, ptr %stringFormat_desc, i32 0, i32 1
  %40 = load i64, ptr %39, align 4
  %41 = trunc i64 %40 to i32
  call void @_lfortran_printf(ptr @1, ptr %38, i32 %41, ptr @0, i32 1)
  call void @_lfortran_free(ptr %33)
  %42 = getelementptr %string_descriptor, ptr %greetings, i32 0, i32 0
  %43 = load ptr, ptr %42, align 8
  call void @_lfortran_printf(ptr @3, ptr %43, i32 25, ptr @2, i32 1)
  call void @_lpython_free_argv()
  br label %return

return:                                           ; preds = %.entry
  br label %FINALIZE_SYMTABLE_string_02

FINALIZE_SYMTABLE_string_02:                      ; preds = %return
  br label %Finalize_Variable_firstname

Finalize_Variable_firstname:                      ; preds = %FINALIZE_SYMTABLE_string_02
  %44 = getelementptr %string_descriptor, ptr %firstname, i32 0, i32 0
  %45 = load ptr, ptr %44, align 8
  call void @_lfortran_free(ptr %45)
  br label %Finalize_Variable_greetings

Finalize_Variable_greetings:                      ; preds = %Finalize_Variable_firstname
  %46 = getelementptr %string_descriptor, ptr %greetings, i32 0, i32 0
  %47 = load ptr, ptr %46, align 8
  call void @_lfortran_free(ptr %47)
  br label %Finalize_Variable_surname

Finalize_Variable_surname:                        ; preds = %Finalize_Variable_greetings
  %48 = getelementptr %string_descriptor, ptr %surname, i32 0, i32 0
  %49 = load ptr, ptr %48, align 8
  call void @_lfortran_free(ptr %49)
  br label %Finalize_Variable_title

Finalize_Variable_title:                          ; preds = %Finalize_Variable_surname
  %50 = getelementptr %string_descriptor, ptr %title, i32 0, i32 0
  %51 = load ptr, ptr %50, align 8
  call void @_lfortran_free(ptr %51)
  ret i32 0
}

declare void @_lpython_call_initial_functions(i32, ptr)

declare ptr @_lfortran_malloc(i64)

declare void @_lfortran_strcpy(ptr, ptr, i8, i8, ptr, i64)

declare ptr @_lcompilers_string_format_fortran(ptr, i64, ptr, ptr, i32, i32, ...)

declare void @_lfortran_printf(ptr, ptr, i32, ptr, i32)

declare void @_lfortran_free(ptr)

declare void @_lpython_free_argv()
