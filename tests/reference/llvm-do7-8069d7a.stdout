; ModuleID = 'LFortran'
source_filename = "LFortran"

define i32 @f(i32* %a) {
.entry:
  %f = alloca i32
  %0 = load i32, i32* %a
  %1 = add i32 %0, 1
  store i32 %1, i32* %f
  br label %return

return:                                           ; preds = %.entry
  %2 = load i32, i32* %f
  ret i32 %2
}

define i32 @main() {
.entry:
  %call_arg_value = alloca i32
  %a = alloca i32
  %i = alloca i32
  store i32 0, i32* %i
  br label %loop.head

loop.head:                                        ; preds = %loop.body, %.entry
  %0 = load i32, i32* %i
  %1 = add i32 %0, 1
  %2 = icmp sle i32 %1, 10
  br i1 %2, label %loop.body, label %loop.end

loop.body:                                        ; preds = %loop.head
  %3 = load i32, i32* %i
  %4 = add i32 %3, 1
  store i32 %4, i32* %i
  store i32 5, i32* %call_arg_value
  %5 = call i32 @f(i32* %call_arg_value)
  store i32 %5, i32* %a
  br label %loop.head

loop.end:                                         ; preds = %loop.head
  ret i32 0
}
