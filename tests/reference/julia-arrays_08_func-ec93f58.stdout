function copy_from_to(a::Array{Int32, 1}, b::Array{Int32, 1})
    local i::Int32 = 0
    for i ∈ 1:length(a)
        b[i] = a[i]
    end
end

function verify(a::Array{Int32, 1}, b::Array{Int32, 1})::Bool
    local i::Int32 = 0
    local r::Bool = false
    r = true
    for i ∈ 1:length(a)
        r = r && a[i] == b[i]
    end
    return r
end

global i::Int32 = 0
global r::Bool = false
global x = Array{Int32, 1}(undef, 10)
global y = Array{Int32, 1}(undef, 10)
for i ∈ 1:length(x)
    x[i] = i
end
copy_from_to(x, y)
r = verify(x, y)
println(r)
if !r
    println(Base.stderr, "ERROR STOP")
    exit(1)
end
