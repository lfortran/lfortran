function copy_from_to(a::Array{Int32, 1}, b::Array{Int32, 1})
    local i::Int32
    for i ∈ 1:length(a)
        b[i] = a[i]
    end
end

function verify(a::Array{Int32, 1}, b::Array{Int32, 1})::Bool
    local i::Int32
    local r::Bool
    r = true
    for i ∈ 1:length(a)
        r = r && a[i] == b[i]
    end
    return r
end

