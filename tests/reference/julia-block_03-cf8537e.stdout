function square(b::Int32)
    local result::Int32 = 0
    result = b * b
    if result ≠ 100
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
    println(result)
end

global a::Int32 = 0
a = 10
@label label_1
let
    b::Int32 = 0
    a = a + 5
    if a == 15
        @goto label_1
    end
    b = a ÷ 2
    square(b)
end
