#include <inttypes.h>

#include <stdlib.h>
#include <stdbool.h>
#include <stdio.h>
#include <string.h>
#include <lfortran_intrinsics.h>


// Implementations
int main(int argc, char* argv[])
{
    _lpython_set_argv(argc, argv);
    int32_t a = 1;
    int32_t b = 2;
    int32_t marks;
    marks = 94;
    if (marks >= 40 + b) {
        printf("%s\n", "Pass!");
    }
    else if (marks <= 39 - a) {
        printf("%s\n", "Failed!");
    }
    else {
        printf("%s\n", "Invalid marks");
    }
    printf("%s%s%d\n", "Your marks are ", " ", marks);
    marks = -1;
    if (marks >= 40 + b) {
        printf("%s\n", "Pass!");
    }
    else if (0 <= marks <= 39 - a) {
        printf("%s\n", "Failed!");
    }
    else {
        printf("%s\n", "Invalid marks");
    }
    printf("%s%s%d\n", "Your marks are ", " ", marks);
    return 0;
}
