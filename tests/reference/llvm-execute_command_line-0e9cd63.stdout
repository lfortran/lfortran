  %0 = getelementptr %string_descriptor, ptr %command, i32 0, i32 0
  %1 = load ptr, ptr %0, align 8
  %2 = getelementptr %string_descriptor, ptr %command, i32 0, i32 0
  %3 = load ptr, ptr %2, align 8
  %4 = getelementptr %string_descriptor, ptr %command, i32 0, i32 1
  %5 = load i64, ptr %4, align 4
  %6 = trunc i64 %5 to i32
  %7 = call i32 @_lfortran_exec_command(ptr %3, i32 %6)
  store i32 %7, ptr %_lcompilers_exit_status, align 4
  br label %return

return:                                           ; preds = %.entry
  br label %FINALIZE_SYMTABLE__lcompilers_execute_command_line_1

FINALIZE_SYMTABLE__lcompilers_execute_command_line_1: ; preds = %return
  ret void
}

define i32 @main(i32 %0, ptr %1) {
.entry:
  %call_arg_value1 = alloca i1, align 1
  %call_arg_value = alloca i1, align 1
  call void @_lpython_call_initial_functions(i32 %0, ptr %1)
  store i1 true, ptr %call_arg_value, align 1
  call void @_lcompilers_execute_command_line_(ptr @string_const, ptr %call_arg_value)
  store i1 true, ptr %call_arg_value1, align 1
  call void @_lcompilers_execute_command_line_1(ptr @string_const.2, ptr %call_arg_value1)
  call void @_lpython_free_argv()
  br label %return

return:                                           ; preds = %.entry
  br label %FINALIZE_SYMTABLE_execute_command_line

FINALIZE_SYMTABLE_execute_command_line:           ; preds = %return
  ret i32 0
}

