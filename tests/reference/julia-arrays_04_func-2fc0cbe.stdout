function abs(a::Float32)::Float32
    local r::Float32
    println("abs")
    if a > Float32(0)
        r = a
    else
        r = -a
    end
    return r
end

function sum(a::Array{Float32, 1})::Float32
    local i::Int32
    local r::Float32
    println("sum")
    r = Float32(0)
    for i âˆˆ 1:length(a)
        r = r + a[i]
    end
    return r
end

function main()
    local a::Array{Float32, 1}
    local b::Float32
    a[1] = Float32(3)
    a[2] = Float32(2)
    a[3] = Float32(1)
    b = sum(Float32(a))
    if abs(Float32(b - Float32(6))) >   9.99999974737875164e-06
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
end

main()
