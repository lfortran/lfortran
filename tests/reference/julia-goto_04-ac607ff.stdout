    @label label_4
    a = a + 40
    @label label_5
    a = a + 50
    @label label_6
    a = a + 60
    if a ≠ 130
        println(Base.stderr, "ERROR STOP")
        exit(1)
    end
    if n * n == 1
        @goto label_7
