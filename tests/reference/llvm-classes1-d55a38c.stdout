; ModuleID = 'LFortran'
source_filename = "LFortran"

%base_class = type <{ ptr, ptr }>
%base = type { i32 }
%string_descriptor = type <{ ptr, i64 }>

@_Name_base = private unnamed_addr constant [5 x i8] c"base\00", align 1
@_Type_Info_base = linkonce_odr unnamed_addr constant { ptr, ptr } { ptr @_Name_base, ptr inttoptr (i64 4 to ptr) }, align 8
@_VTable_base = linkonce_odr unnamed_addr constant { [5 x ptr] } { [5 x ptr] [ptr null, ptr @_Type_Info_base, ptr @_copy_xx_base, ptr @_allocate_struct_xx_base, ptr @__module_xx_show_x] }, align 8
@0 = private unnamed_addr constant [2 x i8] c"\0A\00", align 1
@serialization_info = private unnamed_addr constant [3 x i8] c"I4\00", align 1
@1 = private unnamed_addr constant [5 x i8] c"%s%s\00", align 1
@"ERROR STOP" = private unnamed_addr constant [11 x i8] c"ERROR STOP\00", align 1
@2 = private unnamed_addr constant [2 x i8] c"\0A\00", align 1
@3 = private unnamed_addr constant [5 x i8] c"%s%s\00", align 1

define void @__module_xx_show_x(ptr %this) {
.entry:
  %0 = getelementptr %base_class, ptr %this, i32 0, i32 1
  %1 = load ptr, ptr %0, align 8
  %2 = getelementptr %base, ptr %1, i32 0, i32 0
  store i32 12, ptr %2, align 4
  br label %return

return:                                           ; preds = %.entry
  br label %FINALIZE_SYMTABLE_show_x

FINALIZE_SYMTABLE_show_x:                         ; preds = %return
  ret void
}

define i32 @main(i32 %0, ptr %1) {
.entry:
  %2 = alloca %base_class, align 8
  call void @_lpython_call_initial_functions(i32 %0, ptr %1)
  %b = alloca %base, align 8
  %3 = getelementptr %base, ptr %b, i32 0, i32 0
  %4 = getelementptr %base, ptr %b, i32 0, i32 0
  store i32 10, ptr %4, align 4
  %5 = getelementptr %base_class, ptr %2, i32 0, i32 0
  store ptr getelementptr inbounds ({ [5 x ptr] }, ptr @_VTable_base, i32 0, i32 0, i32 2), ptr %5, align 8
  %6 = getelementptr %base_class, ptr %2, i32 0, i32 1
  store ptr %b, ptr %6, align 8
  call void @__module_xx_show_x(ptr %2)
  %7 = alloca i64, align 8
  %8 = getelementptr %base, ptr %b, i32 0, i32 0
  %9 = load i32, ptr %8, align 4
  %10 = alloca i32, align 4
  store i32 %9, ptr %10, align 4
  %11 = call ptr (ptr, i64, ptr, ptr, i32, i32, ...) @_lcompilers_string_format_fortran(ptr null, i64 0, ptr @serialization_info, ptr %7, i32 0, i32 0, ptr %10)
  %12 = load i64, ptr %7, align 4
  %stringFormat_desc = alloca %string_descriptor, align 8
  %13 = getelementptr %string_descriptor, ptr %stringFormat_desc, i32 0, i32 0
  store ptr %11, ptr %13, align 8
  %14 = getelementptr %string_descriptor, ptr %stringFormat_desc, i32 0, i32 1
  store i64 %12, ptr %14, align 4
  %15 = getelementptr %string_descriptor, ptr %stringFormat_desc, i32 0, i32 0
  %16 = load ptr, ptr %15, align 8
  %17 = getelementptr %string_descriptor, ptr %stringFormat_desc, i32 0, i32 1
  %18 = load i64, ptr %17, align 4
  %19 = trunc i64 %18 to i32
  call void @_lfortran_printf(ptr @1, ptr %16, i32 %19, ptr @0, i32 1)
  %20 = icmp eq ptr %11, null
  br i1 %20, label %free_done, label %free_nonnull

free_nonnull:                                     ; preds = %.entry
  call void @_lfortran_free(ptr %11)
  br label %free_done

free_done:                                        ; preds = %free_nonnull, %.entry
  %21 = getelementptr %base, ptr %b, i32 0, i32 0
  %22 = load i32, ptr %21, align 4
  %23 = icmp ne i32 %22, 12
  br i1 %23, label %then, label %else

then:                                             ; preds = %free_done
  call void @exit(i32 1)
  br label %ifcont

else:                                             ; preds = %free_done
  br label %ifcont

ifcont:                                           ; preds = %else, %then
  call void @_lpython_free_argv()
  br label %return

return:                                           ; preds = %ifcont
  br label %FINALIZE_SYMTABLE_main

FINALIZE_SYMTABLE_main:                           ; preds = %return
  ret i32 0
}

declare void @_lpython_call_initial_functions(i32, ptr)

define linkonce_odr void @_copy_xx_base(ptr %0, ptr %1) {
entry:
  %2 = getelementptr %base, ptr %0, i32 0, i32 0
  %3 = load i32, ptr %2, align 4
  %4 = getelementptr %base, ptr %1, i32 0, i32 0
  br i1 true, label %then, label %else

then:                                             ; preds = %entry
  store i32 %3, ptr %4, align 4
  br label %ifcont

else:                                             ; preds = %entry
  br label %ifcont

ifcont:                                           ; preds = %else, %then
  ret void
}

define linkonce_odr void @_allocate_struct_xx_base(ptr %0) {
entry:
  %1 = call ptr @_lfortran_malloc(i64 16)
  call void @llvm.memset.p0.i32(ptr %1, i8 0, i32 16, i1 false)
  store ptr %1, ptr %0, align 8
  %2 = load ptr, ptr %0, align 8
  store ptr getelementptr inbounds ({ [5 x ptr] }, ptr @_VTable_base, i32 0, i32 0, i32 2), ptr %2, align 8
  %3 = getelementptr %base_class, ptr %2, i32 0, i32 1
  %4 = call ptr @_lfortran_malloc(i64 4)
  call void @llvm.memset.p0.i32(ptr %4, i8 0, i32 4, i1 false)
  store ptr %4, ptr %3, align 8
  %5 = getelementptr %base, ptr %4, i32 0, i32 0
  ret void
}

declare ptr @_lfortran_malloc(i64)

; Function Attrs: nocallback nofree nounwind willreturn memory(argmem: write)
declare void @llvm.memset.p0.i32(ptr nocapture writeonly, i8, i32, i1 immarg) #0

declare ptr @_lcompilers_string_format_fortran(ptr, i64, ptr, ptr, i32, i32, ...)

declare void @_lfortran_printf(ptr, ptr, i32, ptr, i32)

declare void @_lfortran_free(ptr)

