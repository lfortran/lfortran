#include <inttypes.h>

#include <stdlib.h>
#include <stdbool.h>
#include <stdio.h>
#include <string.h>
#include <lfortran_intrinsics.h>


// Implementations
int main(int argc, char* argv[])
{
    _lpython_set_argv(argc, argv);
    int32_t i;
    i = 1;
    while (true) {
        i = i + 1;
        if (i == 10) {
            break;
        }
    }
    if (i != 10) {
        fprintf(stderr, "ERROR STOP");
        exit(1);
    }
    return 0;
}
